{
    "name": "alighorbani1381/tokenable",
    "description": "easy login or signup your user with a token",
    "keywords": ["laravel", "laravel5-package", "PHP", "laravel5"],
    "license": "MIT",
    "homepage": "https://github.com/alighorbani1381/tokenable",
    "authors": [
        {
            "name": "Iman Ghafoori Learned Me!",
            "email": "imanghafoori1@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3|7.2.*|7.3.*|7.4.*",
        "laravel/framework":"~5.1|6.*|7.*|8.*",
        "imanghafoori/laravel-nullable": "^1.2",
        "imanghafoori/laravel-smart-facades": "^1.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "mockery/mockery": "*"
    },
    "autoload": {
        "psr-4": {
            "alighorbani1381\\TwoFactorAuth\\": "src"
        }
